{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2017", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@orbita/orbita-ui/domain-logic": ["libs/orbita-ui/domain-logic/src/index.ts"],
      "@orbita/orbita-ui/feature-employee": ["libs/orbita-ui/feature-employee/src/index.ts"],
      "@orbita/orbita-ui/feature-employee-listing": ["libs/orbita-ui/feature-employee-listing/src/index.ts"],
      "@orbita/orbita-ui/feature-employee-overview": ["libs/orbita-ui/feature-employee-overview/src/index.ts"],
      "@orbita/orbita-ui/feature-empoloyee": ["libs/orbita-ui/feature-empoloyee/src/index.ts"],
      "@orbita/orbita-ui/feature-listing": ["libs/orbita-ui/feature-listing/src/index.ts"],
      "@orbita/orbita-ui/feature-sd-request-overview": ["libs/orbita-ui/feature-sd-request-overview/src/index.ts"],
      "@orbita/orbita-ui/feature-sd-request-wizzard": ["libs/orbita-ui/feature-sd-request-wizzard/src/index.ts"],
      "@orbita/orbita-ui/feature-svt-item": ["libs/orbita-ui/feature-svt-item/src/index.ts"],
      "@orbita/orbita-ui/feature-ticket": ["libs/orbita-ui/feature-ticket/src/index.ts"],
      "@orbita/orbita-ui/shell": ["libs/orbita-ui/shell/src/index.ts"],
      "@orbita/orbita-ui/ui": ["libs/orbita-ui/ui/src/index.ts"],
      "@orbita/orbita-ui/utils": ["libs/orbita-ui/utils/src/index.ts"],
      "@orbita/shared/assets": ["libs/shared/assets/src/index.ts"],
      "@orbita/shared/domain-logic": ["libs/shared/domain-logic/src/index.ts"],
      "@orbita/shared/environment": ["libs/shared/environment/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
