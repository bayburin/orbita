{
  "npmScope": "orbita",
  "affected": { "defaultBase": "master" },
  "implicitDependencies": {
    "package.json": { "dependencies": "*", "devDependencies": "*" },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": { "cacheableOperations": ["build", "lint", "test", "e2e"] }
    }
  },
  "projects": {
    "orbita-ui": {
      "tags": ["type:app", "scope:ticket-system"]
    },
    "orbita-ui-e2e": {
      "tags": ["type:e2e"],
      "implicitDependencies": ["orbita-ui"]
    },
    "ticket-system-domain-logic": {
      "tags": ["scope:ticket-system", "type:domain-logic"]
    },
    "ticket-system-ui": {
      "tags": ["scope:ticket-system", "type:ui"]
    },
    "ticket-system-feature-listing": {
      "tags": ["scope:ticket-system", "type:feature"]
    }
  }
}
